      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@quaver/typescript-config": "workspace:*",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^1.0.41",
    "@quaver/core": "workspace:*",
    "@types/node": "^24.10.3",
    "ai": "^5.0.113",
    "zod": "^4.1.13"
  }
}
